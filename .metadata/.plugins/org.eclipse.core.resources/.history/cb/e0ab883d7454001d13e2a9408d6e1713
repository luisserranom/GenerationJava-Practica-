package cl.generation25102022;

import java.util.HashSet;

public class ArregloHashSet {

	public static void main(String[] args) {
		// definicion seo o hashset
		// HashSet<tipodato> nombre_array = newHash <tipodato>
		HashSet<String> grupo6 = new HashSet<String>();
		
		//2 agregar elementos
		
		
		grupo6.add("javier");
		grupo6.add("julian");
		grupo6.add("francisca");
		grupo6.add("rocio");
		grupo6.add("luis");
		grupo6.add("luis");//esta repitido no agrega
		grupo6.add("LuiS");//sencible mayuscula y minuscula 
		System.out.println(grupo6);
		
		//verificar el contenido del arreglo
		boolean verificacionNombre = grupo6.contains("luis");
		System.out.println("existe ?"+ verificacionNombre);
		//eliminar un elemento por el contenido
		grupo6.remove("luis");
		System.out.println(grupo6);
		//como ver tamaño arreglo
		
		System.out.println("Tamaño de hashset: " + grupo6.size());
	}

}
